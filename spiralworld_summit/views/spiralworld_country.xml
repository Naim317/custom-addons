<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_spiral_world_country_tree" model="ir.ui.view">
            <field name="name">Country</field>
            <field name="model">spiral.world.country</field>
            <field name="arch" type="xml">
                <tree string="Country">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_spiral_world_country_form" model="ir.ui.view">
            <field name="name">Country</field>
            <field name="model">spiral.world.country</field>
            <field name="arch" type="xml">
                <form string="Country">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                                <field name="logo" widget="image"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>

        </record>

        <record id="open_view_spiral_world_country_tree" model="ir.actions.act_window">
            <field name="name">Country</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">spiral.world.country</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Record found!
                </p>
            </field>
        </record>


    </data>


</odoo>
